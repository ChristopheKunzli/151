<table>
    <tr>
        <th>id</th>
        <th>code</th>
        <th>model</th>
        <th>snowLength</th>
        <th>qtyAvailable</th>
        <th>dailyPrice</th>
        <th>photo</th>
        <th>active</th>
    </tr>
    {{#each snows}}
        <tr>
            <th>{{this.id}}</th>
            <th>{{this.code}}</th>
            <th>{{this.model}}</th>
            <th>{{this.snowLength}}</th>
            <th>{{this.qtyAvailable}}</th>
            <th>{{this.dailyPrice}}</th>
            <th>{{this.photo}}</th>
            <th>{{this.active}}</th>
        </tr>
    {{/each}}
</table>